
# block self installing apps
(\.|^)apps\.apple\.com$
#
# block minecraft
(\.|^)minecraft\.net$
^.*minecraft\..*$
# google video
(\.|^)googlevideo\.com$
(\.|^)www\.youtube-nocookie\.com$
(\.|^)youtube\.com$
(\.|^)ytimg\.com$
